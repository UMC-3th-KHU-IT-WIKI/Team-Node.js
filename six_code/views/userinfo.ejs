<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>User info</title>
  </head>
  <body>
    <table>
      <tr>
        <th>이름</th>
        <th>내용</th>
        <th>수정</th>
        <th>삭제</th>
      </tr>
      <% users.forEach(function (value) { %>
      <tr>
        <td><%= value.name%></td>
        <td><%= value.content%></td>
        <td><a href="/edit/<%= value.name %>">수정</a></td>
        <td><a href="/delete/<%= value.name %>">삭제</a></td>
        <td></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
